            <td style="text-align: center;">

                <a href="/advisorImage/madrak/{{madrak}}" download="{{madrak}}">
                    <img class="img-fluid" at style="width: 50px;"
                         src="/advisorimage/madrak/{{madrak}}" alt=""> </a>
            </td>
        </tr>
        {{/each}}
        </tbody>





    </table>





</Div>